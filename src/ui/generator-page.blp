using Gtk 4.0;
using Adw 1;

template $GeneratorPage : Adw.Bin {
  Adw.ViewStack view_stack {
    enable-transitions: true;
    visible-child-name: bind template.active-sub-page;

    Adw.ViewStackPage {
      name: 'starting';
      child: $GeneratorStartingPage starting_page {};

    }

    Adw.ViewStackPage {
      name: 'qr-code-result';
      child: $GeneratorQRCodePage qr_code_page {};
    }
  }
}
